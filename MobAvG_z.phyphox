<?xml version="1.0" encoding="UTF-8"?>
<phyphox version="1.7" locale="it">
    <title>Misura statistica di g lungo z (con filtro)</title>
    <category>Meccanica statistica</category>
    <description>
        Misura l’accelerazione lungo l’asse z (perpendicolare allo schermo).
        Quando il telefono è fermo e orizzontale (schermo in alto) misura circa +9.81 m/s².
        Mostra la distribuzione statistica del rumore (istogramma + gaussiana teorica) e un filtro a media mobile regolabile.
    </description>
    <icon format="base64">
    </icon>
    <color>yellow</color>

    <data-containers>
        <!-- Dati grezzi -->
        <container size="0" static="false" init="">accZ</container>
        <container size="0" static="false" init="">t</container>

        <!-- Dato filtrato con media mobile -->
        <container size="0" static="false" init="">accZ_filtered</container>

        <!-- Parametri regolabili dall'utente -->
        <container size="1" init="80">windowSize</container>     <!-- lunghezza finestra media mobile -->
        <container size="1" init="0.01">binSize</container>     <!-- dimensione bin istogramma -->

        <!-- Statistica -->
        <container size="0">binStart</container>
        <container size="0">binCount</container>
        <container size="1">average</container>
        <container size="1">deviation</container>
        <container size="1">count</container>

        <!-- Gaussiana teorica -->
        <container size="1">minX</container>
        <container size="1">maxX</container>
        <container size="500">gaussianX</container>
        <container size="500">gaussian</container>
    </data-containers>

    <input>
        <sensor type="accelerometer" rate="0">
            <output component="z">accZ</output>
            <output component="t">t</output>
        </sensor>
    </input>

    <analysis sleep="0" onUserInput="false">
        <!-- Media mobile regolabile -->
        <moving_average>
            <input clear="false">accZ</input>
            <input as="n" clear="false">windowSize</input>
            <output clear="true">accZ_filtered</output>
        </moving_average>

        <!-- Istogramma -->
        <binning>
            <input clear="false">accZ</input>
            <input as="dx" clear="false">binSize</input>
            <output clear="true">binStart</output>
            <output clear="true">binCount</output>
        </binning>

        <!-- Statistica -->
        <average>
            <input clear="false">accZ</input>
            <output as="average">average</output>
            <output as="stddev">deviation</output>
        </average>
        <count>
            <input clear="false">accZ</input>
            <output as="count">count</output>
        </count>

        <!-- Range per la gaussiana -->
        <subrange>
            <input clear="false">binStart</input>
            <input as="from" type="value">0</input>
            <input as="length" type="value">1</input>
            <output>minX</output>
        </subrange>
        <max>
            <input as="y" clear="false">binStart</input>
            <output as="max">maxX</output>
        </max>
        <ramp>
            <input as="start" clear="false">minX</input>
            <input as="stop" clear="false">maxX</input>
            <output>gaussianX</output>
        </ramp>

        <!-- Curva gaussiana teorica normalizzata correttamente -->
        <formula formula="[4]*[5]*exp(-0.5*([1_]-[2])^2/[3]^2)/(2.506628*[3])">
            <input clear="false">gaussianX</input>
            <input clear="false">average</input>
            <input clear="false">deviation</input>
            <input clear="false">binSize</input>
            <input clear="false">count</input>
            <output>gaussian</output>
        </formula>
    </analysis>

    <views color="E0E040">
        <view label="Misura statistica di g lungo z">
            <!-- Grafico temporale con segnale grezzo e filtrato -->
            <graph label="g_z(t) – grezzo e filtrato" aspectRatio="2.5" style="lines" lineWidth="1" partialUpdate="true" history="1"
                   labelX="tempo" labelY="g_z" unitX="s" unitY="m/s²" yPrecision="4">
                <input axis="x">t</input>
                <input axis="y" color="4080FF">accZ</input>                    <!-- grezzo (blu) -->
                <input axis="y" style="lines" lineWidth="3" color="FF0000">accZ_filtered</input> <!-- filtrato (rosso) -->
            </graph>

            <!-- Istogramma + gaussiana teorica -->
            <graph label="Distribuzione statistica" labelX="g_z" labelY="Conteggi" unitX="m/s²" xPrecision="4" partialUpdate="false">
                <input axis="x">binStart</input>
                <input axis="y" style="vbars" color="E0E040" lineWidth="0.9">binCount</input>
                <input axis="x">gaussianX</input>
                <input axis="y" style="lines" lineWidth="2" color="FF0000">gaussian</input>
            </graph>

            <!-- Controlli utente -->
            <edit label="Finestra media mobile" unit="punti" default="80" min="10" max="500" signed="false">
                <output>windowSize</output>
            </edit>
            <edit label="Dimensione bin" unit="m/s²" default="0.01" signed="false">
                <output>binSize</output>
            </edit>

            <!-- Valori numerici -->
            <value label="g_z (istantaneo)" precision="4" unit="m/s²">
                <input>accZ</input>
            </value>
            <value label="g_z filtrato" precision="4" unit="m/s²">
                <input>accZ_filtered</input>
            </value>
            <value label="Media" precision="4" unit="m/s²">
                <input>average</input>
            </value>
            <value label="Deviazione standard" precision="4" unit="m/s²">
                <input>deviation</input>
            </value>
            <value label="Conteggi" precision="0">
                <input>count</input>
            </value>
        </view>
    </views>

    <export>
        <set name="Dati grezzi">
            <data name="Tempo (s)">t</data>
            <data name="g_z grezzo (m/s²)">accZ</data>
            <data name="g_z filtrato (m/s²)">accZ_filtered</data>
        </set>
        <set name="Istogramma e statistica">
            <data name="Media g_z (m/s²)">average</data>
            <data name="Dev. std. g_z (m/s²)">deviation</data>
            <data name="Bin start (m/s²)">binStart</data>
            <data name="Bin count">binCount</data>
        </set>
    </export>
</phyphox>
